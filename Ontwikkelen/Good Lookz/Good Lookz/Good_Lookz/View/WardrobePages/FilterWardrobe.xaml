<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Good_Lookz.View.WardrobePages.FilterWardrobe"
             Title="Filter wardrobe">
  <StackLayout VerticalOptions="FillAndExpand">
    <Picker x:Name="pType" Title="Choose a type of clothing" SelectedIndexChanged="pType_SelectedIndexChanged"/>
    <Picker x:Name="pColour" Title="Choose a colour"/>
  </StackLayout>
</ContentPage>